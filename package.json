{
  "name": "admin",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "api": "json-server --host 0.0.0.0 --port 8081 --watch json-server/db.json --routes json-server/routes.json",
    "apiSlow": "json-server --delay 5000 --host 0.0.0.0 --port 8081 --watch json-server/db.json --routes json-server/routes.json"
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.17",
    "autoprefixer": "^10.4.14",
    "daisyui": "^2.51.6",
    "json-server": "^0.17.3",
    "pinia": "^2.0.35",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.0",
    "@types/node": "^18.16.3",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.3.2",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4",
    "vite": "^4.3.4",
    "vue-tsc": "^1.6.4"
  }
}
